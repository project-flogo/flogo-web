{
  "name": "flogo-web",
  "version": "0.1.0",
  "description": "Wonderful web application for Flogo",
  "repository": {
    "url": "https://github.com/TIBCOSoftware/flogo-web.git"
  },
  "scripts": {
    "start": "gulp",
    "gulp": "gulp",
    "clean": "rm -rfv `find . -path \"*/node_modules\" -print`",
    "webdriver-manager": "webdriver-manager update && webdriver-manager start",
    "webdriver-manager-update": "webdriver-manager update",
    "e2e": "cd specs && protractor protractor.conf.js",
    "e2e-container": "cd specs && docker run -it --rm --net=host -e FLOGO_HOST=\"$FLOGO_HOST\" -e FLOGO_PORT=\"$FLOGO_PORT\" --name flogo-protractor-runner -v $(pwd):/protractor/project hortonworks/docker-e2e-protractor protractor.conf.js"
  },
  "author": "Shaoke Xu<shaoxu@tibco.com>",
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-preset-node6": "^11.0.0",
    "babel-register": "^6.9.0",
    "browser-sync": "2.13.0",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-changed": "^1.3.0",
    "gulp-concat": "^2.5.2",
    "gulp-filter": "^4.0.0",
    "gulp-help": "^1.6.1",
    "gulp-inject": "^4.1.0",
    "gulp-inline-ng2-template": "^3.0.1",
    "gulp-less": "^3.1.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-nodemon": "^2.0.7",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "1.6.0",
    "gulp-typescript": "^3.0.2",
    "gulp-uglify": "^1.2.0",
    "less": "^2.7.1",
    "mustache": "^2.2.1 ",
    "pouchdb-server": "",
    "protractor": "^4.0.2",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.5",
    "swagger-jsdoc": "^1.7.0",
    "systemjs-builder": "^0.15.32",
    "tslint": "^3.5.0",
    "typescript": "~2.0.10",
    "vinyl-fs": "^2.4.3",
    "webdriver-manager": "^10.2.1"
  },
  "dependencies": {}
}
